    <script>
        function addToWishlist(productId){
            fetch('/add-whishlist-product', {
                method: 'POST', 
                headers:{
                    'Content-Type' : 'application/json'
                },
                body: JSON.stringify({productId})
            })
            .then((response) => response.json())
            .then((data) => {
                if(data.success){
                    // Swal.fire({
                    //     icon: 'success',
                    //     title: 'Add WishList...ðŸ¤©'
                    // })
                    window.location.reload()
                }else{
                    if(data.redirecturl){
                        window.location.href = data.redirecturl;
                    }else{
                        Swal.fire({
                    icon: 'error',
                    title: 'Oooh My God..!',
                    text: data.message || 'Failed to add to wishlist.',
                });
                    }
                }
            }) 
            .catch((error) => {
                console.log('AddToWishList Front:', error);
            }) 
        }
    </script>
